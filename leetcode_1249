    const S = s.split("")
    let len = S.length, stack = []
    for (let i = 0, c = S[0]; i < len; c = S[++i])
        if (c === ")")
            if (stack.length) stack.pop()
            else delete S[i]
        else if (c === "(") stack.push(i)
    for (let i = 0; i < stack.length; i++)
        delete S[stack[i]]
    return S.join("")
